#1
for i in range(1,6):
    for j in range(1, i+1):
        print("*", end="")
    print()
    
    
#2
for i in range(1,6):
    for j in range(1,7-i):
        print("*", end="")
    print()
    
#3
for i in range(0,6):
    for j in range(0, 5-i):
        print(' ', end='')
    for j in range(0, 2*i-1):
        print('*', end='')
    print()
